{
  "expo": {
    "name": "Align Body",
    "slug": "align-body",
    "version": "1.4.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#1F2D16"
    },
    "updates": {
